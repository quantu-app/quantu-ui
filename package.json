{
	"name": "quantu",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"tauri": "tauri",
		"vite": "vite",
		"start": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"desktop": "tauri dev",
		"desktop.build": "tauri build",
		"openapi-generator-quantu": "rm -rf src/lib/openapi/quantu && openapi-generator-cli generate -i http://localhost:3000/api/docs.json -g typescript-fetch -o 'src/lib/openapi/quantu' --type-mappings=DateTime=Date --additional-properties=modelPropertyNaming=snake_case,,supportsES6=true,withInterfaces=true",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"engine": {
		"node": ">= 18.12.1"
	},
	"devDependencies": {
		"@openapitools/openapi-generator-cli": "^2.5.2",
		"@playwright/test": "^1.31.2",
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.10.0",
		"@tauri-apps/api": "next",
		"@tauri-apps/cli": "next",
		"@types/js-cookie": "^3.0.3",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.54.0",
		"@typescript-eslint/parser": "^5.54.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"internal-ip": "^7.0.0",
		"postcss-import": "^15.1.0",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.4",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"vitest": "^0.29.2"
	},
	"dependencies": {
		"@aicacia/debounce": "^0.1.21",
		"@aicacia/json": "^0.1.16",
		"@svelte-put/clickoutside": "^2.0.0",
		"eventemitter3": "^5.0.0",
		"idb": "^7.1.1",
		"js-cookie": "^3.0.1",
		"slate": "^0.91.4",
		"slate-history": "^0.86.0",
		"svelte-icons": "^2.1.0",
		"svelte-localstorage-writable": "^0.1.0",
		"svelte-portal": "^2.2.0",
		"svelte-slate": "^0.1.78",
		"uuid": "^9.0.0"
	}
}
